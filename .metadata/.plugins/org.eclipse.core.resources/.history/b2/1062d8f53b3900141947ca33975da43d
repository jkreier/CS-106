
public class loancalculator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double loan = 20000.0;
		double intrestrate = 0.1;
		double paymentnumber = 12;
		double interestamount = loan * intrestrate;
		System.out.println(interestamount * paymentnumber);		
		
	}

}
